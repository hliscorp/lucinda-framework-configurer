{
    "name": "lucinda/framework-configurer",
    "description": "Console step by step configurer for Lucinda Framework 3.0",
    "homepage": "https://github.com/aherne/lucinda-framework-configurer",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucian Gabriel Popescu",
            "email": "lucian.gabriel.popescu@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lucinda\\Configurer\\": "src/"
        }
    },
    "require": {
        "php": "^7.1|^8.0",
        "ext-simplexml": "*",
        "ext-xml": "*",
        "ext-readline": "*",
        "lucinda/framework-engine": "~2.0",
        "lucinda/logging": "~3.0",
        "lucinda/nosql-data-access": "~3.0",
        "lucinda/oauth2-client": "~2.0",
        "lucinda/security": "~3.0",
        "lucinda/mvc": "~3.0",
        "lucinda/sql-data-access": "~3.0",
        "lucinda/view-language": "~3.0",
        "lucinda/internationalization": "~3.0",
        "lucinda/errors-mvc": "~2.0",
        "lucinda/headers": "~1.0",
        "lucinda/abstract_mvc": "~1.0",
        "lucinda/migrations": "~1.0"
    },
    "require-dev": {
        "lucinda/unit-testing": "^1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/hliscorp/errors-api"
        },
        {
            "type": "vcs",
            "url": "https://github.com/hliscorp/lucinda-framework-engine"
        },
        {
            "type": "vcs",
            "url": "https://github.com/hliscorp/php-internationalization-api"
        },
        {
            "type": "vcs",
            "url": "https://github.com/hliscorp/php-logging-api"
        },
        {
            "type": "vcs",
            "url": "https://github.com/hliscorp/migration"
        },
        {
            "type": "vcs",
            "url": "https://github.com/hliscorp/php-servlets-api"
        },
        {
            "type": "vcs",
            "url": "https://github.com/hliscorp/php-nosql-data-access-api"
        },
        {
            "type": "vcs",
            "url": "https://github.com/hliscorp/oauth2client"
        },
        {
            "type": "vcs",
            "url": "https://github.com/hliscorp/php-security-api"
        },
        {
            "type": "vcs",
            "url": "https://github.com/hliscorp/php-sql-data-access-api"
        },
        {
            "type": "vcs",
            "url": "https://github.com/hliscorp/unit-testing"
        },
        {
            "type": "vcs",
            "url": "https://github.com/hliscorp/php-view-language-api"
        },
        {
            "type": "vcs",
            "url": "https://github.com/hliscorp/mvc"
        },
        {
            "type": "vcs",
            "url": "https://github.com/hliscorp/headers-api"
        }
    ]
}
